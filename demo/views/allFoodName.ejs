<!-- POST、GET前端測試 -->
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>DAFAULT</title>
        <style type="text/css">
            .box{
                width:800px;
                height:400px;
                border:1px solid #000;
                margin: 0 auto;
                background:-webkit-linear-gradient(top, skyblue, white)
            }
            h1{
                text-align: center;
            }
            input{
                font-size:20px;
            }
        </style>
    </head>
    <body>
        <div class="box">
            <h1>訂單</h1>
           <p>
               已訂餐的電話：
           </p>
           <url>
               <% 
                    for(var i=0;i<allPhomeNum.length;i++) {
               %>
                        <li>
                            <a href="/orderFood/<%= allPhomeNum[i]%>">
                                <%= allPhomeNum[i]%>
                            </a>
                        </li>
               <% 
                    }
               %>
           </url>
        </div>        
    </body>
</html>
